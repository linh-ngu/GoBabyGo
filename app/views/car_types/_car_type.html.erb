<div id="<%= dom_id car_type %>">
  <table class="border border-gray-900 bg-gray-300">
    <tbody>
      <tr>
        <th class="p-2">Name:</th>
        <td class="p-2"><%= car_type.name %></td>
      </tr>
      <tr>
        <th class="p-2">Price:</th>
        <td class="p-2">$<%= car_type.price %></td>
      </tr>
      <tr>
        <th class="p-2">Max Height:</th>
        <td class="p-2"><%= car_type.max_height %> in.</td>
      </tr>
      <tr>
        <th class="p-2">Max Weight:</th>
        <td class="p-2"><%= car_type.max_weight %> lbs.</td>
      </tr>
    </tbody>
  </table>
</div>
