<h1>UserApplications#edit</h1>

<div class="user_application edit">
    <h2>Edit Application</h2>

    <%= form_for(@user_application) do |f| %>

        <%= render(partial: 'form', locals: {f: f}) %>

        <div class="form-buttons">
            <%= f.submit("Edit Application") %>
        </div>

    <% end %>
</div>