<h1>UserApplications#new</h1>

<div class="user_application new">
    <h2>New Application</h2>

    <%= form_for @user_application do |f| %>
        
        <%= render(partial: 'form', locals: {f: f}) %>

        <div class="form-buttons">
        <%= f.submit("Submit Application") %>
        </div>

    <%end%>
</div>
